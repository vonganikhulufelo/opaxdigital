  <label for="name">pick_up_qty</label>
</div>
</div>

<div class="form-group">
<div class="form-label-group">
  <%= text_field_tag 'pick_up_value', nil, placeholder: 'pick_up_value', class: "form-control pick_up_value", required: "required" %>

  <label for="name">pick_up_value</label>
</div>
</div>

<div class="form-group">
<div class="form-label-group">
  <%= text_field_tag 'bol_reference', nil, placeholder: 'bol_reference', class: "form-control bol_reference", required: "required" %>

